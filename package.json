{
  "name": "tic-tac-toe",
  "version": "1.0.0",
  "scripts": {
    "go": "node build/src/driver",
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "all": "npm run build && npm run go",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "engines": {
    "node": ">= 10.13 <11"
  },
  "dependencies": {
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "enum-values": "^1.2.1",
    "immutable": "^4.0.0-rc.12",
    "jest": "~23.6.0",
    "lodash": "^4.17.11",
    "prettier": "1.15.3",
    "rimraf": "^2.6.3",
    "ts-jest": "~23.10.5",
    "tslib": "~1.9.3",
    "tslint": "~5.12.0",
    "tslint-config-prettier": "1.17.0",
    "tslint-microsoft-contrib": "~6.0.0",
    "tsutils": "~3.7.0",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.123",
    "tslib": "~1.9.3"
  }
}
